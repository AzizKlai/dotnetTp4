@{
    ViewData["Title"] = "courses";
}

<div class="text-center">
    <h1 class="display-4">@ViewData["Title"]</h1>
</div>
@model List<String> 
    @if(Model.Count!=0){
        
  <table class="table">
    <thead>
        <tr>
             <th >course</th>
             <th >enrolled students</th>
           
        </tr>
        </thead>
        <tbody>
         
           
            @foreach (var s in Model)
            { 
        <tr>
            
            <th>@s</th>
            <th><a href="/Home/Course/@s">...</a></th>
            </tr>
 

           
        }
        
    </tbody>
</table>      
    }

