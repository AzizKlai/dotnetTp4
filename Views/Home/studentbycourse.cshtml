@{
   

}

<div class="text-center">
    <h1 class="display-4">@ViewData["Title"]</h1>
</div>


@model List<tp4.Models.Student> 
    @if(Model.Count!=0){
        
  <table class="table">
    <thead>
        <tr>
             <th>id</th>
            <th>firstName</th>
            <th>lastname</th>
            <th>phone Number</th>
            <th>timestamp</th>
            <th>university</th>
        </tr>
        </thead>
        <tbody>
         
           
            @foreach (var student in Model)
            { 
        <tr>
            <th>@student.id</th>
            <th>@student.first_name</th>
            <th>@student.last_name</th>
            <th>@student.phone_number</th>
            <th>@student.timestamp</th>
            <th>@student.university</th>
        </tr>
 

           
        }
        
    </tbody>
</table>      
    }
